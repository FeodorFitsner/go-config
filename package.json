{
  "name": "go-runtime",
  "description": "Detects installed go (golang) runtime(s) and makes them available for use by other packages.",
  "keywords": [
    "go",
    "golang",
    "gopath",
    "gb",
    "godep"
  ],
  "main": "./lib/main",
  "version": "0.2.0",
  "repository": "https://github.com/joefitzgerald/go-runtime",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.1.0 <2.0.0"
  },
  "author": "Joe Fitzgerald <jfitzgerald@pivotal.io> (http://github.com/joefitzgerald)",
  "homepage": "https://atom.io/packages/go-runtime",
  "bugs": {
    "url": "https://github.com/joefitzgerald/go-runtime/issues"
  },
  "dependencies": {
    "fs-plus": "^2.8.1",
    "glob": "^5.0.14",
    "lodash": "^3.10.1",
    "os-homedir": "^1.0.1"
  },
  "devDependencies": {
    "fs-extra": "^0.24.0",
    "underscore-plus": "1.x",
    "eslint": "^1.5.0",
    "babel-eslint": "^4.1.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "temp": "^0.8.3",
    "touch": "^1.0.0"
  },
  "providedServices": {
    "go.runtime": {
      "versions": {
        "0.1.0": "provide"
      }
    }
  },
  "consumedServices": {
    "environment": {
      "versions": {
        "0.1.0": "consumeEnvironment"
      }
    }
  }
}
